{
  "expo": {
    "name": "Rentik",
    "slug": "rentik",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#032B3C"
    },
    "scheme": "rentik",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.rentik",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/CarLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.rentik",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Rentik necesita tu ubicación para mostrarte vehículos cercanos y completar tu perfil."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/Logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "googleWebClientId": "YOUR_GOOGLE_WEB_CLIENT_ID",
      "googleIosClientId": "YOUR_GOOGLE_IOS_CLIENT_ID",
      "googleAndroidClientId": "YOUR_GOOGLE_ANDROID_CLIENT_ID",
      "stripePublishableKey": "YOUR_STRIPE_PUBLISHABLE_KEY",
      "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
    }
  }
}
