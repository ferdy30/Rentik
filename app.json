{
  "expo": {
    "name": "Rentik",
    "slug": "rentik",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "rentik",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.rentik"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/CarLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.rentik"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
